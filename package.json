  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open": "npx cypress open",
    "cy:run": "npm run prereport && npx cypress run",
    "delete:reports": "rm -r cypress/results/* || true",
    "prereport": "npm run delete:reports",
    "delete:results": "node clean.js",
    "mochawesome:html": "npx mochawesome-merge \"cypress/results/mochawesome/*.json\" > mochawesome.json && npx marge mochawesome.json",
    "junit:report": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/result.xml",
    "cypress:e2e": "npm run cy:run && npm run junit:report && npm run mochawesome:html"
  },
